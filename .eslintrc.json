{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
    "linebreak-style": "off",
    "indent": "off",
    "no-unused-vars": "warn",
    "no-plusplus": "off"
  }
}
